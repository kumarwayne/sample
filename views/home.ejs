            <a href="/live"><h1 style="color:brown"><%=data.battingteam1%>  <em><%=data.runs1%>/<%=data.wkt1%></em> </h1><br></a>
            <h6><%=data.status1%></h6>
       </div>
       <div class="carousel-item">
          <!-- <h4>%=srs2%>(%=mnum2%>)</h4><br>
          <h1 style="color:green">%=battingteam2%>  <em>%=runs2%>/%=wkt2%></em> </h1><br>
          <h6>%=status2%></h6> -->
          <h2><em>OOPS !!! NO other Live matches</em></h2>
          <h1>:-(</h1>
      </div>
     <%})%>
    <%}else{%>
